<?xml version="1.0" encoding="UTF-8"?>
<solver>
    <solutionClass>com.fghifarr.tarkamleague.configs.planner.PlannerSolution</solutionClass>
    <entityClass>com.fghifarr.tarkamleague.configs.planner.MatchPlannerV2</entityClass>

    <scoreDirectorFactory>
        <constraintProviderClass>com.fghifarr.tarkamleague.configs.planner.PlannerConstraintProvider</constraintProviderClass>
    </scoreDirectorFactory>

    <termination>
        <terminationCompositionStyle>OR</terminationCompositionStyle>
        <minutesSpentLimit>30</minutesSpentLimit>
        <unimprovedSpentLimit>PT2M30S</unimprovedSpentLimit>
        <bestScoreFeasible>true</bestScoreFeasible>
    </termination>
</solver>
